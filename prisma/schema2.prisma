// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/brave"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_BRAVE_URL")
}

model Partner {
  id            Int     @id @default(autoincrement())
  name          String
  email         String?
  phone         String?
  comment       String?
  payment_terms String?

  @@map("partners")
}
